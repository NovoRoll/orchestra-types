    "module": "commonjs",
    "rootDir": "./src",
    "declaration": true,
    "sourceMap": true,
    "removeComments": true,
    "outDir": "./dist",
    "strict": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true
  },
  "exclude": ["./lib, './dist"],
  "include": ["./src"]
}
